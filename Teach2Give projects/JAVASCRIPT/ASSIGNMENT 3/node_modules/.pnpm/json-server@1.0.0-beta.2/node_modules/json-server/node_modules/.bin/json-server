#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jotham/Desktop/internship/Teach2Give projects/JAVASCRIPT/ASSIGNMENT 3/node_modules/.pnpm/json-server@1.0.0-beta.2/node_modules/json-server/lib/node_modules:/home/jotham/Desktop/internship/Teach2Give projects/JAVASCRIPT/ASSIGNMENT 3/node_modules/.pnpm/json-server@1.0.0-beta.2/node_modules/json-server/node_modules:/home/jotham/Desktop/internship/Teach2Give projects/JAVASCRIPT/ASSIGNMENT 3/node_modules/.pnpm/json-server@1.0.0-beta.2/node_modules:/home/jotham/Desktop/internship/Teach2Give projects/JAVASCRIPT/ASSIGNMENT 3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jotham/Desktop/internship/Teach2Give projects/JAVASCRIPT/ASSIGNMENT 3/node_modules/.pnpm/json-server@1.0.0-beta.2/node_modules/json-server/lib/node_modules:/home/jotham/Desktop/internship/Teach2Give projects/JAVASCRIPT/ASSIGNMENT 3/node_modules/.pnpm/json-server@1.0.0-beta.2/node_modules/json-server/node_modules:/home/jotham/Desktop/internship/Teach2Give projects/JAVASCRIPT/ASSIGNMENT 3/node_modules/.pnpm/json-server@1.0.0-beta.2/node_modules:/home/jotham/Desktop/internship/Teach2Give projects/JAVASCRIPT/ASSIGNMENT 3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/bin.js" "$@"
else
  exec node  "$basedir/../../lib/bin.js" "$@"
fi
